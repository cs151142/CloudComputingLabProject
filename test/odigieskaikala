docker-compose up --build //trexei

docker ps -a //vlepoume containers

http://localhost:8080 //vscode afou treksei

docker volume ls //vlepoume ola ta volumes

docker volume inspect volumename //info gia to volume

docker-compose down //stamataei, to volume synexizei na yparxei

docker exec -it ruby-container sh //mpainoume sto container tis ruby gia na kanoume run kati
